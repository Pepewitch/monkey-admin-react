{
  "name": "monkey-admin-react",
  "productName" : "Monkey Admin",
  "version": "0.1.0",
  "description":"coding by react framework",
  "private": true,
  "author": "Witch",
  "license": "MONKEY-MONKEY",
  "build":{
    "appId": "com.example.electron-cra",
    "files": [
	    "build/**/*",
	    "node_modules/**/*"
	],
	"directories":{
	    "buildResources": "assets"
	}
  },
  "dependencies": {
    "chartist": "^0.10.1",
    "d3": "^4.13.0",
    "downshift": "^1.28.0",
    "electron-is-dev": "^0.3.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "material-ui": "1.0.0-beta.30",
    "material-ui-icons": "1.0.0-beta.17",
    "moment": "^2.20.1",
    "needle": "^2.1.1",
    "npm-run-all": "^4.1.1",
    "perfect-scrollbar": "^1.3.0",
    "query-string": "^5.1.0",
    "react": "^16.2.0",
    "react-chartist": "^0.13.1",
    "react-datepicker": "^1.2.1",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.0",
    "react-swipeable-views": "^0.12.12",
    "socket.io-client": "^2.0.4",
    "whatwg-fetch": "^2.0.3"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm start \" \"wait-on http://localhost:3000 && electron .\"",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "electron-pack": "build --config.extraMetadata.main=build/electron.js"
  },
  "devDependencies": {
    "electron": "^1.8.2"
  },
  "postinstall": "electron-builder install-app-deps",
  "preelectron-pack": "yarn build",
  "homepage": "./"
}
